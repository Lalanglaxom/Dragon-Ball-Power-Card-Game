config_version=5

[application]

config/name="Dragon Ball Power Card Game"
config/description="Bộ bài sức mạnh Dragon Ball"
run/main_scene="res://scenes/game.tscn"
config/features=PackedStringArray("4.2")
config/icon="res://icon.png"

[autoload]

GlobalSignal="*res://scenes/global_signal.gd"

[display]

window/size/viewport_height=752

[editor_plugins]

enabled=PackedStringArray("res://addons/simple_card_pile_ui/plugin.cfg")

[file_customization]

folder_colors={
"res://card/": "yellow",
"res://environment/": "green"
}

[input]

