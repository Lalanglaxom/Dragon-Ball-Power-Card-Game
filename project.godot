config_version=5

[application]

config/name="Dragon Ball Power Card Game"
config/description="Bộ bài sức mạnh Dragon Ball"
run/main_scene="res://scenes/3d/playground.tscn"
config/features=PackedStringArray("4.3")
config/icon="res://icon.png"

[autoload]

GlobalManager="*res://scenes/GlobalManager.gd"

[display]

window/size/always_on_top=true
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/Asset_Drawer/plugin.cfg", "res://addons/anthonyec.camera_preview/plugin.cfg", "res://addons/cyberassets/plugin.cfg", "res://addons/simple_card_pile_ui/plugin.cfg")

[file_customization]

folder_colors={
"res://card/": "yellow",
"res://environment/": "green"
}

[input]

flip={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":32,"key_label":0,"unicode":32,"location":0,"echo":false,"script":null)
]
}
space={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":32,"key_label":0,"unicode":32,"location":0,"echo":false,"script":null)
]
}

[rendering]

textures/canvas_textures/default_texture_filter=0
anti_aliasing/quality/msaa_2d=3
