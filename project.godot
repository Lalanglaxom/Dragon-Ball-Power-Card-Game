; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Dragon Ball Power Card Game"
config/description="Bộ bài sức mạnh Dragon Ball"
run/main_scene="res://scenes/main_menu.tscn"
config/features=PackedStringArray("4.3")
config/icon="res://icon.png"

[autoload]

Global="*res://scenes/scripts/Global.gd"

[display]

window/size/always_on_top=true
window/stretch/mode="canvas_items"

[editor]

movie_writer/movie_file="E:/Videos/Captures/godot/power card game.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/Asset_Drawer/plugin.cfg", "res://addons/Todo_Manager/plugin.cfg", "res://addons/YouCanDoIt/plugin.cfg", "res://addons/anthonyec.camera_preview/plugin.cfg", "res://addons/cyberassets/plugin.cfg", "res://addons/godot_state_charts/plugin.cfg")

[file_customization]

folder_colors={
"res://card/": "yellow",
"res://environment/": "green",
"res://players/": "red",
"res://scenes/": "orange",
"res://ui/": "purple"
}

[input]

flip={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":32,"key_label":0,"unicode":32,"location":0,"echo":false,"script":null)
]
}
space={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":32,"key_label":0,"unicode":32,"location":0,"echo":false,"script":null)
]
}

[rendering]

textures/canvas_textures/default_texture_filter=2
anti_aliasing/quality/msaa_2d=3
anti_aliasing/quality/msaa_3d=2
anti_aliasing/quality/use_taa=true
anti_aliasing/quality/use_debanding=true
